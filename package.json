{
  "name": "eloquente-ui",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs": "pnpm --filter docs -- build",
    "changeset": "changeset",
    "publish": "changeset publish",
    "version": "changeset version",
    "clean": "turbo run clean"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@turbo/gen": "^2.0.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "turbo": "^2.0.3"
  },
  "packageManager": "pnpm@8.6.10",
  "engines": {
    "node": ">=18"
  }
}
