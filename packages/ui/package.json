{
  "name": "@eloquent/ui",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "private": false,
  "files": [
    "dist",
    "tailwind.config.js",
    "postcss.config.js",
    "styles/global.css"
  ],
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./tailwind": "./tailwind.config.js",
    "./postcss": "./postcss.config.js",
    "./style": {
      "import": "./tw.css",
      "require": "./tw.css"
    }
  },
  "scripts": {
    "lint": "eslint src --cache",
    "typecheck": "tsc -b",
    "build:css": "npx tailwindcss -o tw.css",
    "build:cjs": "tsup --format cjs --external react",
    "build:esm": "tsup --format esm --external react",
    "build": "npm run clean && concurrently \"npm run build:cjs\"  \"npm run build:esm\"",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest  --ui",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^9.4.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.4",
    "tsconfig": "workspace:*",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@eloquent/react": "workspace:*",
    "@eloquent/tokens": "workspace:*",
    "@eloquent/styles": "workspace:*",
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@storybook/addon-docs": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.1.5",
    "input-otp": "^1.2.4",
    "jsdom": "^24.1.0",
    "lucide-react": "^0.394.0",
    "next-themes": "^0.3.0",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.51.5",
    "react-resizable-panels": "^2.0.19",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
